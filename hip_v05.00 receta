turtles-own [nivel_tech nivel_energia soymujer? innovando eratrans?]  ;; define variables propias de tortugas
patches-own [sustrato-tech sustrato-energia proto-color proto-level]  ;; define variables propias parcelas
globals [nmax x_acumulado ticks_reset_jerarquico num_agentes]
;; iniciar el modelo
to setup
  clear-all
  set num_agentes numero_inicial_agentes
  set ticks_reset_jerarquico 0
  set x_acumulado 0
  set nmax 0
  set layout? false
  set creo_link? false
  set convierto_color? false
  set permeabilidad-muralla-in 0
  set permeabilidad-muralla-out 0
  crear-muralla ;; crea una muralla para el vector open
  ask patches [
    set sustrato-tech random-float 5 ;; asigna al azar sustrato tec
    set sustrato-energia random-float 10.0 ;; asigna al azar sustrato energia
    recolor-grass ;; convierte los valores anteriores en colores
  ]
  crear-hubs ;; crea super hubs tec o energia
  ;;crea los agentes
  create-turtles numero_inicial_agentes * (1 - %dentro) [
    ifelse random 2 < 1[
    set xcor random-x-fuera-muralla
      set ycor 50 - random-float 100
      set soymujer? true
      set eratrans? false
    ]
    [
    set ycor random-y-fuera-muralla
    set xcor 50 - random-float 100
      set soymujer? false
      set eratrans? false
    ]

    ;;setxy random-y-fuera-muralla
    set color random-float 140
    ;;set shape "person"
    set nivel_tech 0
    set nivel_energia 50
  ]

    create-turtles numero_inicial_agentes * (%dentro) [

    set xcor (random-float x-muralla * (1 - random 3))
    set ycor (random-float y-muralla * (1 - random 3))


    ;;setxy random-y-fuera-muralla
    set color random-float 140
    ;;set shape "person"
    set nivel_tech 0
    set nivel_energia 50
  ]

  asigna_trans
  reset-ticks
end


to go
  if ticks = ticks_reset_jerarquico + 30 [create-turtles 1 [ ;; creo 1 numero_inicial_agentes + 1
    set xcor 0
    set ycor 30
    set size 3

  ]]

  if ticks = ticks_reset_jerarquico + 60 [ ;;creo 10 numero_inicial_agentes + 2 a +11
    let n 1
    repeat 10 [
    create-turtles 1 [
    set xcor -10 + n * 2
    set ycor 10
    set size 2
      ]
    set n n + 1]]

    if ticks = ticks_reset_jerarquico + 90 [ ;;creo 10 numero_inicial_agentes + 2 a +11
    let n 1
    repeat 30 [
    create-turtles 1 [
    set xcor -15 + n
    set ycor -10
    set size 2
      ]
    set n n + 1]]


    if ticks = ticks_reset_jerarquico + 120 [ ;;creo 10 numero_inicial_agentes + 2 a +11
    let n 1
    repeat 60 [
    create-turtles 1 [
    set xcor -30 + n
    set ycor -30
    set size 2
     ]
    set n n + 1]]


    if ticks = ticks_reset_jerarquico + 150 [
    let n 1
    repeat 10 [
    ask turtle (num_agentes + 1) [
     create-link-with turtle (num_agentes + 1 + n) ]
    set n n + 1
    ]
    set n 1
    repeat 10 [
    ask turtle (num_agentes + 1 + n) [
     create-link-with turtle (num_agentes + 1 + 10 + n * 3 - 2)
     create-link-with turtle (num_agentes + 1 + 10 + 1 + n * 3 - 2)
     create-link-with turtle (num_agentes + 1 + 10 + 2 + n * 3 - 2)]
    set n n + 1
    ]
    set n 1
    repeat 30 [
    ask turtle (num_agentes + 1 + 10 + n) [
     create-link-with turtle (num_agentes + 1 + 10 + 30 + n * 2 - 1)
     create-link-with turtle (num_agentes + 1 + 10 + 30 + 1 + n * 2 - 1)
    set n n + 1
    ]
    ]
  ]
  if ticks = ticks_reset_jerarquico + 200 [
    let n 1
    repeat 101[
      ask turtle (num_agentes + n) [ask link-neighbors [if random-float 100.0 < 90 [set color [color] of myself ]]]
    set n n + 1
  ]
]
  if ticks = ticks_reset_jerarquico + 220 [ask turtles [set layout? true]]
  if ticks = ticks_reset_jerarquico + 250 [ask turtles [
    set nivel_energia 200
    set convierto_color? true
    set creo_link? true
    set permeabilidad-muralla-in 2
    set permeabilidad-muralla-out 2

  ]]




  ;;set x_acumulado x_acumulado + count turtles with [shape = "x"]
  ;;update-clusters
  ask turtles [
    ask my-links [set color [color] of myself ]
    ifelse verinnova? [ifelse shape = "x" [show-turtle][hide-turtle]][show-turtle]

    let discriminada false
    if soymujer? = true and feminismo? = true and random-float 1 > feminismo [set discriminada true]
    gira        ;; pide girar a los agentes
    move          ;; mueve los agentes (y verificara si están quietos o cruzan muralla)
    if expulsion? = true and ticks > ticks_reset_jerarquico + 250 [expulsion] ;; verifica si un agente esta muerto y lo elimina
    eat           ;; el agente come energia y tec del sustrato
    if discriminada = false [color_co];; si cumplen las condiciones conversa color
    if discriminada = false [link_co] ;; si cumplen las condiciones crea links
    haz_proto
  ]
  reload_acelerador_tech_publico
  reload_acelerador_energia_publico
  reload_energia_fuera_muralla
  regrow-sustrato-energia
  regrow-sustrato-tech
  resize-nodes


  if layout? = true [layout]
  tick
  my-update-plots ;; plot the population counts
end

;; recolor the grass to indicate how much has been eaten
to recolor-grass
  set pcolor black
  if capa_tech?[
    set pcolor scale-color green sustrato-tech 0 20]
  if capa_energia?[
    set pcolor scale-color red sustrato-energia 0 20]

  if proto-level > 1[
    set proto-level proto-level - 1
    set pcolor scale-color proto-color proto-level 0 turnos_dura_proto]
end

;; regrow the grass
to regrow-sustrato-energia
  ask patches [
    set sustrato-energia sustrato-energia + ratio-reposicion-recursos
    if sustrato-energia > 10.0 [
      set sustrato-energia 10.0
    ]
    recolor-grass
  ]
end
;; regrow the grass
to regrow-sustrato-tech
    ask patches [
    set sustrato-tech sustrato-tech + random-float 5 - random-float 5
    if sustrato-tech > 10.0 [
      set sustrato-tech 10.0
    ]
   recolor-grass
  ]
end

;; sheep procedure, sheep eat grass
to eat
  ;; check to make sure there is grass here
  if ( sustrato-energia >= nivel_energia-gain-from-grass ) [
    ;; increment the sheep's nivel_tech
    set nivel_energia nivel_energia + nivel_energia-gain-from-grass
    ;; decrement the grass
    set sustrato-energia sustrato-energia - nivel_energia-gain-from-grass
    recolor-grass
  ]

    ;; check to make sure there is grass here
  if ( sustrato-tech >= 1 ) [
    ;; increment the sheep's nivel_tech
    set nivel_tech nivel_tech + 1
    ;; decrement the grass
    set nivel_tech (nivel_tech - 0.5)

  ]
end

;; asks those sheep with no nivel_tech to die
to expulsion
  if nivel_energia <= 0  [
    if count my-links > 0 [ask my-links [die]]
    set xcor random-x-fuera-muralla
    set ycor random-y-fuera-muralla
  ]
  ;;if random-float 1000 < 10  [
  ;;  if count my-links > 0 [ask my-links [die]]
  ;;  set xcor random-x-fuera-muralla
  ;;  set ycor random-y-fuera-muralla
  ;;]
end

;; update the plots in the interface tab
to my-update-plots
  ;;plot count turtles
end

;; sheep procedure, the sheep changes its heading
to gira
  ;; gira a izquierda o derecha de forma aleatoria con media recto
  rt random 90
  lt random 90
end

;; sheep procedure, the sheep moves which costs it nivel_tech
to move
  ifelse nivel_energia > nivel_energia_quieto [
    set nivel_energia nivel_energia - movement-cost ;; reduce the nivel_tech by the cost of movement
    let cruzo? cruzolamuralla?
    if cruzo? = "in" [if permeabilidad-muralla-in <= random 100 [rt 180]]
    if cruzo? = "out" [ifelse permeabilidad-muralla-out <= random 100 [rt 180][ask n-of (p_romper_link_al_salir * count my-links) my-links [die]]]
    forward 1
  ]
  [set nivel_energia nivel_energia - coste_quieto] ;; reduce the nivel_tech by the cost of movement]
end

to-report cruzolamuralla?
  if shape = "person" [report "no"] ;; si soy trans la muralla no existe para mi
  let x_actual xcor
  let y_actual ycor
  forward 1
  let x_futura xcor
  let y_futura ycor
  forward -1
  ifelse abs y_actual >= y-muralla or abs x_actual >= x-muralla [;;estoy fuera
    ifelse abs y_futura >= y-muralla or abs x_futura >= x-muralla[report "no"];;sigo fuera
    [report "in"]];; entro dentro
  [ifelse abs y_futura >= y-muralla or abs x_futura >= x-muralla[report "out"];;estoy dentro voy fuera
    [report "no"]];;estoy dentro sigo dentro
  ;;report "in"
  ;;report "out"
  report "no"
end



to reload_energia_fuera_muralla

  ask patches with [parcela_interior = false][
    ;; give grass to the patches, color it shades of green
    set sustrato-energia 10
    recolor-grass ;; change the world green
   ]

end

to-report parcela_interior
  ifelse abs pycor > y-muralla or abs pxcor > x-muralla [report false];;estoy fuera
  [report true];;estoy dentro
end
to-report tortuga_interior
  ifelse abs ycor > y-muralla or abs xcor > x-muralla [report false];;estoy fuera
  [report true];;estoy dentro
end



to reload_acelerador_tech_publico
  ask patches with [pxcor <= 5 and pxcor > -1 * 5 and pycor < 5 and pycor >= -1 * 5][
    ;; give grass to the patches, color it shades of green
    recolor-grass ;; change the world green
   ]
ask patches with [pxcor <= acelerador_tech_publico and pxcor > -1 * acelerador_tech_publico and pycor < acelerador_tech_publico and pycor >= -1 * acelerador_tech_publico][
    ;; give grass to the patches, color it shades of green
    set sustrato-tech 10
    recolor-grass ;; change the world green
   ]

end
to reload_acelerador_energia_publico
  ask patches with [pxcor <= 5 and pxcor > -1 * 5 and pycor < 5 and pycor >= -1 * 5][
    ;; give grass to the patches, color it shades of green
    recolor-grass ;; change the world green
   ]
ask patches with [pxcor <= acelerador_energia_publico and pxcor > -1 * acelerador_energia_publico and pycor < acelerador_energia_publico and pycor >= -1 * acelerador_energia_publico][
    ;; give grass to the patches, color it shades of green
    set sustrato-energia 10
    recolor-grass ;; change the world green
   ]
end

to color_co
  if not tortuga_interior and random-float 100 < diversidad [set color random 149];;diversidad
   ;; interact with Von Neumann neighborhood
  if tortuga_interior and random 100 < probabilidad_co  [
  ask turtles-on neighbors [
      if tortuga_interior[
    if color != [color] of myself and convierto_color? = true  [
      set color [color] of myself
  ]]]]
end

to link_co
if tortuga_interior and random 100 < probabilidad_link and count my-links < max_links and count links < max_total_links [
  ask turtles-on neighbors [
      if tortuga_interior[
        if color = [color] of myself and creo_link? = true [
          create-link-with myself
  ]]]]
    if tortuga_interior and random 100 < probabilidad_link and shape = "person" and count my-links < max_links_trans[ ;;soy trans y creo links con los que no son de mi color y con limite de links x3
      ask turtles-on neighbors [
      create-link-with myself
      ]
    ]
  ifelse not verlinks? [ask my-links [hide-link]][ask my-links [show-link]]

end


to resize-nodes
  ifelse size_nodes? [ask turtles [ set size sqrt (count my-links + 1) ]]
  [ask turtles [ set size 1 ]]
end

to layout
  ;; the number 3 here is arbitrary; more repetitions slows down the
  ;; model, but too few gives poor layouts
  if count turtles with [tortuga_interior = true] > 0[
  repeat 3 [
    ;; the more turtles we have to fit into the same amount of space,
    ;; the smaller the inputs to layout-spring we'll need to use
    let factor sqrt count turtles with [tortuga_interior = true]
    ;; numbers here are arbitrarily chosen for pleasing appearance
    layout-spring turtles with [tortuga_interior = true] links (tautness / factor) (zero * 7 / factor) (repulsion / factor)




    display  ;; for smooth animation
  ]
  ;; don't bump the edges of the world
  let x-offset max [xcor] of turtles with [tortuga_interior = true] + min [xcor] of turtles with [tortuga_interior = true]
  let y-offset max [ycor] of turtles with [tortuga_interior = true] + min [ycor] of turtles with [tortuga_interior = true]
  ;; big jumps look funny, so only adjust a little each time
  set x-offset limit-magnitude x-offset 0.1
  set y-offset limit-magnitude y-offset 0.1
  ask turtles with [tortuga_interior = true] [ setxy (xcor - x-offset / 2) (ycor - y-offset / 2) ]
  ]
end

to-report limit-magnitude [number limit]
  if number > limit [ report limit ]
  if number < (- limit) [ report (- limit) ]
  report number
end

to crear-muralla

  let cx 0                ;; x coordinate of patch you want to circle
  let cy 0                ;; y coordinate of patch you want to circle
  crt 1 [                 ;; create a single drawing turtle
    set heading 0
    setxy -1 * x-muralla -1 * y-muralla       ;; move it to the highlight patch + the radius
    pd                    ;; put the pen down
    set color white
    repeat 2 * y-muralla [forward 1]
    right 90
    repeat 2 * x-muralla [forward 1]
    right 90
    repeat 2 * y-muralla [forward 1]
    right 90
    repeat 2 * x-muralla [forward 1]
    die                   ;; remove the turtle
  ]

end

to-report random-x-fuera-muralla
  let min_ x-muralla
  let max_ 50
  let signo 1
  if random-float 2 < 1 [set signo -1]
    report (min_ + random (max_ - min_))* signo
end

to-report random-y-fuera-muralla
  let min_ y-muralla
  let max_ 50
    let signo 1
  if random-float 2 < 1 [set signo -1]
    report (min_ + random (max_ - min_)) * signo
end

to crear-hubs
  ask patches with [pxcor <= acelerador_tech_publico and pxcor > -1 * acelerador_tech_publico and pycor < acelerador_tech_publico and pycor >= -1 * acelerador_tech_publico][
    ;; give grass to the patches, color it shades of green
    set sustrato-tech 10
    recolor-grass ;; change the world green
  ]
  ask patches with [pxcor <= acelerador_energia_publico and pxcor > -1 * acelerador_energia_publico and pycor < acelerador_energia_publico and pycor >= -1 * acelerador_energia_publico][
    ;; give grass to the patches, color it shades of green
    set sustrato-energia 10
    recolor-grass ;; change the world green
  ]
end
to asigna_trans
    ;; designa los agentes trans
  ifelse count turtles with [shape = "person"] < (porcentaje_trans * numero_inicial_agentes / 100)[ ;; si el numero de trans < los que yo quiero
   ask n-of (porcentaje_trans * numero_inicial_agentes / 100 - count turtles with [shape = "person"]) turtles [
      if shape != "x" [set shape "person"]
    ]
  ] ;;creo mas

  [ ;; si el numero de trans > los que yo quiero
    ask n-of (count turtles with [shape = "person"] - porcentaje_trans * numero_inicial_agentes / 100) turtles with [shape = "person"] [
      if shape != "x" [set shape "default"]
    ] ;;cambio color sobran
  ]
end

to haz_proto
  if proto-level > 1 [
    set color proto-color
  ]

  if count my-links > 3 [
  if tortuga_interior and random 100 < probabilidad_proto * 10[
    set proto-color color
    set proto-level turnos_dura_proto]
  ]
  ;;proto-color
  ;;proto-level
end

to kill_popes
  ask turtles [
    if count my-links > max_links_trans * 0.5 [
    if count my-links > 0 [ask my-links [die]]
    set xcor random-x-fuera-muralla
    set ycor random-y-fuera-muralla
    ]]
end

to update-clusters
  asigna_trans

  ask turtles [
    let n1 0
    let n2 0
    if innovando > 0 [set innovando innovando - 1]
    if innovando = 0 and shape = "x" [ifelse eratrans? = true [set shape "person" set eratrans? false][set shape "default"]]
    ;;let my-points links with [ shade-of? color [ color ] of myself ]
    set n1 count my-links with [shade-of? color [ color ] of myself]
    ask link-neighbors with[shade-of? color [ color ] of myself][
      let my-n2 count my-links with [shade-of? color [ color ] of myself]
      set n2 (n2 + my-n2)]
    ;;if n1 > 8 and n2 > 20
    if n1 + n2 > nmax [set nmax (n1 + n2)]
    if n1 + n2 > umbral_innova [
      if shape = "trans"[set eratrans? true]
      set shape "x"
      set innovando 10
      ask link-neighbors with [shade-of? color [ color ] of myself][set shape "x" set innovando 10 if shape = "trans"[set eratrans? true]
        ask link-neighbors with [shade-of? color [ color ] of myself][set shape "x" set innovando 10 if shape = "trans"[set eratrans? true]]
      ]
    ]
    stop
 ]
end
